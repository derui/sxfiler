{
  "name": "sxfiler",
  "version": "1.0.0",
  "description": "This is a **Simple** and **Extensible** , and having *Two screen* filer created by Electron.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "cpx": "^1.5.0",
    "electron": "1.7.5",
    "electron-packager": "^8.7.2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "stylus": "^0.54.5",
    "webpack": "^3.5.5"
  },
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "build:css": "node ./scripts/build-css.js",
    "build:main": "node ./scripts/build-ocaml.js",
    "bundle": "node ./scripts/bundle-main.js",
    "bundle:prod": "node ./scripts/bundle-main.js prod",
    "package": "node ./scripts/package-electron.js",
    "package:linux": "node ./scripts/package-electron.js linux",
    "run:test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test": "npm-run-all -s build bundle:test run:test",
    "format": "node ./scripts/format.js",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derui/sxfiler.git"
  },
