{
  "name": "sxfiler",
  "version": "0.5.0",
  "description": "This is a **Simple** and **Extensible** , and having *Two screen* filer created by Electron.",
  "main": "src/electron-starter.js",
  "directories": {
    "test": "test"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/ts/**/*.{ts,tsx}",
      "!node_modules/"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/ts/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "watchPathIgnorePatterns": [
      "node_modules"
    ]
  },
  "scripts": {
    "start": "BROWSER=none webpack-dev-server --config config/webpack.development.js",
    "build:prepare": "node scripts/prepare-build.js",
    "build:bundle": "webpack --config config/webpack.production.js",
    "build": "npm-run-all build:prepare build:bundle",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "electron": "ELECTRON_DISABLE_SANDBOX=1 ELECTRON_START_URL=http://localhost:3000 electron .",
    "package": "node ./scripts/create-package.js",
    "format": "node ./scripts/format.js",
    "clean": "rimraf -r build dist",
    "dev": "npm-run-all -p start electron",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "generate-commands": "node scripts/generate-commands.js",
    "install-hook": "cpx .git-hooks/pre-commit .git/hooks/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derui/sxfiler.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derui/sxfiler/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@storybook/addon-info": "^5.2.0",
    "@storybook/addon-knobs": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.8",
    "@types/node-int64": "^0.4.29",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/react-transition-group": "^4.2.2",
    "@types/shallowequal": "^1.1.1",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "ajv": "^6.10.0",
    "babel-loader": "^8.0.6",
    "bfj": "6.1.1",
    "big-integer": "^1.6.40",
    "chokidar": "^2.0.4",
    "core-js": "3.2.1",
    "cpx": "^1.5.0",
    "dotenv": "7.0.0",
    "dotenv-expand": "5.1.0",
    "electron": "^6.0.9",
    "electron-packager": "^14.0.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint_d": "^8.0.0",
    "file-loader": "4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.4.0",
    "fs-extra": "7.0.1",
