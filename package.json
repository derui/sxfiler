{
  "name": "sxfiler",
  "version": "1.0.0",
  "description": "This is a **Simple** and **Extensible** , and having *Two screen* filer created by Electron.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "ajv": "^5.5.2",
    "chokidar": "^2.0.3",
    "cpx": "^1.5.0",
    "electron": "^2.0.2",
    "electron-packager": "^12.1.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.9.0",
    "node-sass-glob-importer": "^5.1.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "puppeteer": "^1.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "snap-shot-it": "^4.0.1",
    "username": "^3.0.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "scripts": {
    "start": "npm run watch",
    "app": "node ./scripts/run-app.js",
    "watch": "npm-run-all -p watch:css watch:main",
    "build": "npm-run-all -s build:dll build:main build:css",
    "build:css": "node ./scripts/build-css.js",
    "build:main": "node ./scripts/build-ocaml.js",
    "build:test": "node ./scripts/build-ocaml-test.js",
    "build:dll": "node ./scripts/build-dll.js",
    "build:prod": "npm-run-all -s build:prod:dll build:prod:main",
    "build:prod:main": "node ./scripts/build-ocaml.js prod",
    "build:prod:dll": "node ./scripts/build-dll.js prod",
    "watch:css": "node ./scripts/build-css.js watch",
    "watch:main": "node ./scripts/build-ocaml.js watch",
    "package": "node ./scripts/package-electron.js",
    "test:library": "node ./scripts/run-test.js",
    "test": "npm-run-all -s build:test test:library",
    "format": "node ./scripts/format.js",
    "clean": "node ./scripts/clean.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derui/sxfiler.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derui/sxfiler/issues"
  },
  "homepage": "https://github.com/derui/sxfiler#readme"
}
