    "node-int64": "^0.4.0",
    "npm-run-all": "^4.1.3",
    "pnp-webpack-plugin": "1.4.3",
    "prettier": "^1.17.0",
    "prettier_d": "^5.7.4",
    "puppeteer": "^1.20.0",
    "react": "^16.8.3",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.3",
    "react-test-renderer": "^16.8.3",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.0",
    "shallowequal": "^1.1.0",
    "styled-components": "^4.3.2",
    "terser-webpack-plugin": "1.2.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.3",
    "url-loader": "2.1.0",
    "uuid": "^3.3.2",
    "webpack": "4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.0"
  }
}
