{
  "name": "sxfiler",
  "version": "1.0.0",
  "description": "This is a **Simple** and **Extensible** , and having *Two screen* filer created by Electron.",
  "main": "src/electron-starter.js",
  "directories": {
    "test": "test"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/ts/stories/**/*"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "./node_modules/jest-watch-typeahead/filename.js",
      "./node_modules/jest-watch-typeahead/testname.js"
    ],
    "watchPathIgnorePatterns": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@storybook/addon-info": "^4.1.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "@types/node-int64": "^0.4.29",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__addon-info": "^4.1.0",
    "@types/storybook__addon-knobs": "^4.0.1",
    "@types/storybook__react": "^4.0.1",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "ajv": "^5.5.2",
    "babel-loader": "^8.0.5",
    "chokidar": "^2.0.4",
    "classnames": "^2.2.6",
    "cpx": "^1.5.0",
    "create-react-app": "^2.1.1",
    "electron": "^3.0.0-beta.5",
    "electron-packager": "^12.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "intersection-observer": "^0.5.1",
    "karma": "^4.0.1",
    "karma-electron": "^6.0.0",
    "karma-webpack": "^3.0.5",
    "lodash.isequal": "^4.5.0",
    "node-int64": "^0.4.0",
    "node-sass": "^4.9.1",
    "node-sass-glob-importer": "^5.1.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.4",
    "puppeteer": "^1.11.0",
    "react": "^16.6.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.8.3",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^3.3.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "BROWSER=none node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "package": "node ./scripts/package-electron.js",
    "format": "node ./scripts/format.js",
    "clean": "node ./scripts/clean.js",
    "dev": "npm-run-all -p start electron",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
