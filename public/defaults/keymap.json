{
  "bindings": [
    {"action": "interactive.quit", "key": "q", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.cursor-up", "key": "k", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.cursor-down", "key": "j", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.change-side", "key": "Tab", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.up-directory", "key": "Backspace", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.reload-all", "key": "R", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.open-node", "key": "Enter", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.toggle-mark", "key": "Space", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.move", "key": "m", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.delete", "key": "d", "contexts": ["onFileTree"]},
    {"action": "interactive.filer.copy", "key": "c", "contexts": ["onFileTree"]},
    {"action": "interactive.history.open", "key": "h", "contexts": ["onFileTree"]},
    {"action": "interactive.history.select", "key": "Enter", "contexts": ["onCompletion", "forHistory"]},
    {"action": "interactive.keymap.reload", "key": "R", "contexts": ["onFileTree"]},
    {"action": "interactive.task.selectNextReply", "key": "ArrowDown", "contexts": ["onSuggestion"]},
    {"action": "interactive.task.selectPreviousReply", "key": "ArrowUp", "contexts": ["onSuggestion"]},
    {"action": "interactive.task.selectPreviousReply", "key": "ArrowUp", "contexts": ["onSuggestion"]},
    {"action": "interactive.task.sendReply", "key": "Enter", "contexts": ["onSuggestion"]},
    {"action": "interactive.task.cancel", "key": "Escape", "contexts": ["onSuggestion"]},
    {"action": "interactive.finder.open", "key": "f", "contexts": ["onFileTree"]},
    {"action": "interactive.finder.confirm", "key": "Enter", "contexts": ["onCompletion", "forFinder"]},
    {"action": "interactive.finder.close", "key": "Escape", "contexts": ["onCompletion", "forFinder"]},
    {"action": "interactive.completer.cursor-up", "key": "ArrowUp", "contexts": ["onCompletion"]},
    {"action": "interactive.completer.cursor-up", "key": "C-p", "contexts": ["onCompletion"]},
    {"action": "interactive.completer.cursor-down", "key": "ArrowDown", "contexts": ["onCompletion"]},
    {"action": "interactive.completer.cursor-down", "key": "C-n", "contexts": ["onCompletion"]},
    {"action": "interactive.bookmark.toggle", "key": "C-b", "contexts": ["onFileTree"]}
  ]
}
