{
  "bindings": [
    {"action": "builtin.quit", "key": "q", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.moveCursorDown", "key": "j", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.moveCursorUp", "key": "k", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.changePaneSide", "key": "Tab", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.moveParent", "key": "^", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.enterDirectory", "key": "Enter", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.toggleMark", "key": "Space", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.move", "key": "m", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.delete", "key": "d", "contexts": ["onFileTree"]},
    {"action": "builtin.filer.copy", "key": "c", "contexts": ["onFileTree"]},
    {"action": "builtin.history.open", "key": "h", "contexts": ["onFileTree"]},
    {"action": "builtin.history.select", "key": "Enter", "contexts": ["onCompletion", "forHistory"]},
    {"action": "builtin.keymap.reload", "key": "R", "contexts": ["onFileTree"]},
    {"action": "builtin.task.selectNextReply", "key": "ArrowDown", "contexts": ["onSuggestion"]},
    {"action": "builtin.task.selectPreviousReply", "key": "ArrowUp", "contexts": ["onSuggestion"]},
    {"action": "builtin.task.selectPreviousReply", "key": "ArrowUp", "contexts": ["onSuggestion"]},
    {"action": "builtin.task.sendReply", "key": "Enter", "contexts": ["onSuggestion"]},
    {"action": "builtin.task.cancel", "key": "Escape", "contexts": ["onSuggestion"]},
    {"action": "builtin.finder.open", "key": "f", "contexts": ["onFileTree"]},
    {"action": "builtin.finder.select", "key": "Enter", "contexts": ["onCompletion", "forFinder"]},
    {"action": "builtin.completer.close", "key": "Escape", "contexts": ["onCompletion"]},
    {"action": "builtin.completer.cursorUp", "key": "ArrowUp", "contexts": ["onCompletion"]},
    {"action": "builtin.completer.cursorDown", "key": "ArrowDown", "contexts": ["onCompletion"]},
    {"action": "builtin.bookmark.toggle", "key": "C-b", "contexts": ["onFileTree"]}
  ]
}
