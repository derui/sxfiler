(jbuild_version 1)

(alias
 ((name runtest)
  (deps (test_sxfiler_server.exe))
  (action (run ${<}))))

(executable
 ((name test_sxfiler_server)
  (flags (-safe-string))
  (libraries (alcotest alcotest-lwt
              lwt
              sxfiler.server.task
              sxfiler.server.core
              sxfiler.server
              sxfiler.core
  ))
  (preprocess (pps (lwt_ppx)))))
