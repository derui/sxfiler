(jbuild_version 1)

(alias
 ((name js)
  (deps (sxfiler_renderer_entry.bc.js sxfiler_main_entry.bc.js))))

(executable
 ((name sxfiler_renderer_entry)
  (modules sxfiler_renderer_entry)
  (libraries (sxfiler.renderer))
  (js_of_ocaml ((flags ("--pretty"))))
  (preprocess (pps (js_of_ocaml-ppx ppx_variants_conv)))))

(executable
 ((name sxfiler_main_entry)
  (modules sxfiler_main_entry)
  (libraries (sxfiler.main))
  (js_of_ocaml ((flags ("--pretty"))))
  (preprocess (pps (js_of_ocaml-ppx ppx_variants_conv)))))
