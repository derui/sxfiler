{
  "bindings": [
    {"action": "file_list:next_item", "key": "j", "when": {"onFileTree": true}},
    {"action": "file_list:prev_item", "key": "k", "when": {"onFileTree": true}},
    {"action": "file_list:swap_scanner", "key": "Tab", "when": {"onFileTree": true}},
    {"action": "completion:prev_item", "key": "ArrowUp", "when": {"onCompletion": "true"}},
    {"action": "completion:next_item", "key": "ArrowUp", "when": {"onCompletion": "true"}},
    {"action": "completion:select_item", "key": "Enter", "when": {"onCompletion": "true"}}

  ]
}
