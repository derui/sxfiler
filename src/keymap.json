{
  "bindings": [
    {"action": "builtin.filer.moveCursorDown", "key": "j", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.moveCursorUp", "key": "k", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.changePaneSide", "key": "Tab", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.moveParent", "key": "^", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.enterDirectory", "key": "Enter", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.toggleMark", "key": "Space", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.move", "key": "m", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.delete", "key": "d", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.filer.copy", "key": "c", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.history.open", "key": "h", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.history.select", "key": "Enter", "when": {contexts: ["onCompletion", "forHistory"]}},
    {"action": "builtin.history.close", "key": "Escape", "when": {contexts: ["onCompletion", "forHistory"]}},
    {"action": "builtin.history.cursorUp", "key": "ArrowUp", "when": {contexts: ["onCompletion", "forHistory"]}},
    {"action": "builtin.history.cursorDown", "key": "ArrowDown", "when": {contexts: ["onCompletion", "forHistory"]}},
    {"action": "builtin.keymap.reload", "key": "R", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.task.selectNextReply", "key": "ArrowUp", "when": {contexts: ["onSuggestion"]}},
    {"action": "builtin.task.selectPrevReply", "key": "ArrowDown", "when": {contexts: ["onSuggestion"]}},
    {"action": "builtin.finder.open", "key": "f", "when": {contexts: ["onFileTree"]}},
    {"action": "builtin.finder.select", "key": "Enter", "when": {contexts: ["onCompletion", "forFinder"]}},
    {"action": "builtin.finder.close", "key": "Escape", "when": {contexts: ["onCompletion", "forFinder"]}},
    {"action": "builtin.finder.cursorUp", "key": "ArrowUp", "when": {contexts: ["onCompletion", "forFinder"]}},
    {"action": "builtin.finder.cursorDown", "key": "ArrowDown", "when": {contexts: ["onCompletion", "forFinder"]}}
  ]
}
